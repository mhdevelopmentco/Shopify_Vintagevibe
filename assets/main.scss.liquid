  top: 14px;
}

.jcf-list .jcf-option
{
  padding: 10px 14px 10px 20px;
}

.search_bar .location-left .search_limit
{
  display: none !important;
}

.search_bar .location-right .main_search_label
{
  margin: 0 0 18px 48px !important;
}

.search_bar .location-right .search_filters
{
  width: 100% !important;
  padding: 0 !important;
  border: 0 !important;
  margin: 0 0 0 48px !important;
}

.search_bar button
{
  background: 0 !important;
  color: #000000 !important;
  font-family: neuzeit-grotesk, Arial, Helvetica, sans-serif !important;
  font-size: 20px !important;
  font-weight: normal !important;
  white-space: nowrap !important;
  width: 175px !important;
  padding: 20px 30px 35px !important;
  border: 3px solid #000000 !important;
  -webkit-border-radius: 54px !important;
  -moz-border-radius: 54px !important;
  border-radius: 54px !important;
  margin: 0 -webkit-calc(50% - 87.5px) 0 !important;
  margin: 0 -moz-calc(50% - 87.5px) 0 !important;
  margin: 0 calc(50% - 87.5px) 0 !important;
  outline-style: none;
  -moz-box-shadow: none !important;
  -webkit-box-shadow: none !important;
  box-shadow: none !important;
  text-shadow: none !important;
}

.search_bar button:hover
{
  color: #f33900 !important;
  border: 3px solid #f33900 !important;
}

.search_bar button:active
{
  top: 0 !important;
}

.search_bar .search_within_distance .distance_label, .search_bar .search_limit .search_limit_label
{
  padding-right: 4px !important;
}

.search_bar .search_within_distance .within_distance, .search_bar #within_distance, .search_bar #limit
{
  width: auto !important;
}

.addresses li
{
  font-size: 15px !important;
  line-height: 19px !important;
  padding-top: 5px !important; padding-bottom: 5px !important;
}

.addresses li .distance
{
  margin: 40px 0 0 0 !important;
}

.addresses li a
{
  margin: 0 0 15px 10px !important;
}

@media only screen and (max-width: 920px) and (min-width: 769px)
{
  .search_bar .search_within_distance, .search_bar .search_limit
