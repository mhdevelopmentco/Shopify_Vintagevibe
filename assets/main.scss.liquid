{
  .search_bar
  {
    padding: 48px 24px 36px !important;
  }
  
  .search_bar .location-left, .search_bar .location-right
  {
    width: 100% !important;
  }
  
  .search_bar .location-left .search_within_distance
  {
    padding: 0 !important;
  }
  
  .search_bar .location-right .main_search_label
  {
    margin: 0 0 24px 0 !important;
  }
  
  .search_bar .location-right .search_filters
  {
    margin: 0 !important;
  }
