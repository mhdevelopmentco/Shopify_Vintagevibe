    locationLeft.append('<span class="main_search_label">View by your location:</span>');
    locationLeft.append(searchBar.find('.main_search_bar'));
    locationLeft.append(searchBar.find('.search_within_distance'));
    locationLeft.append(searchBar.find('.search_limit'));
    locationRight.append('<span class="main_search_label">View by country:</span>');
    locationRight.append(searchBar.find('.search_filters'));
    
    searchBar.find('.search_filter input').each(function()
    {
      $(this).attr({'type' : 'radio', 'name' : 'countries'});
    });
  }
  
  $('iframe.store-locator').load( function()
  {
    setTimeout(function()
    {
      resizeIframe();
    }, 40);
    
    $(this).contents().find('.addresses ul').on('DOMNodeInserted, DOMNodeRemoved ','li', function()
    {
      resizeIframe();
    });
    
    $(window).on('resize', function()
    {
      resizeIframe();
    });
    
    function resizeIframe()
    {
      var iframe = $('iframe.store-locator');
      var contents = iframe.contents();
      iframe.css('height', '');
      iframe.css('height', contents.find('body').prop('scrollHeight'));
      iframe.css('visibility', '');
    }
  });
  
  if ($('body').attr('id') != 'page-index') { return; }
  
  $(window).load(function()
  {
    adjustMargin();
  });
  
  $(window).on('resize', function()
  {
    adjustMargin();
  });
  
  function adjustMargin()
  {
    if ($(window).width() > 1008)
    {
      $('.info-col').each(function()
      {
        var col = $(this);
        var tab = $(this).parent().find('.tabs-box');
        if (col.height() >= tab.height())
        {
          tab.css('margin-top', (col.height() - tab.height()) + 'px');
          col.css('margin-top', '0px');
        }
        else (tab.height() > col.height())
        {
          col.css('margin-top', (tab.height() - col.height()) + 'px');
          tab.css('margin-top', '0px');
        }
      });
    }
    else
    {
      $('.info-col').each(function()
      {
        var col = $(this);
        var tab = $(this).parent().find('.tabs-box');
        col.css('margin-top', '0px');
        tab.css('margin-top', '0px');
      });
    }
  }
  
});