    $(window).on('scroll', function()
    {
      pageTitle.css({ 'opacity' : Math.max(0, Math.min(1, (($(this).scrollTop()-fadeStart)/(fadeEnd-fadeStart)*(0-1) + 1))) });
    });
  });
}
  
