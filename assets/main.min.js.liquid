function createBanner()
{
  var promo = $('.promo-block');
  var imageUrl = promo.css('background-image').match(/^url\("?(.+?)"?\)$/);
  
  if (imageUrl[1])
  {
    imageUrl = imageUrl[1];
    var image = new Image();
    image.src = imageUrl;
      
    $(image).one('load', function()
    {
      if (!$('body[id*="page-article"]').length) { promo.css('visibility', ''); return; }
      
      adjustBanner(image.width, image.height);
      promo.css('visibility', '');
        
      $(window).on('resize', function()
      {
        adjustBanner(image.width, image.height);
      });
        
      function adjustBanner(width, height)
      {
        promo.css('height', '');
        
        var minWidth  = promo.width();
        var minHeight = promo.height();
          
        height = minWidth*(height/width);
        width  = minWidth;
          
        if (height < minHeight)
        {
          width  = minHeight*(width/height);
          height = minHeight;
        }
          
        width  = Math.floor((height+60)*(width/height));
        height = Math.floor((height+60));
          
        promo.css('cssText', promo.attr('style') + ' height: ' + height + 'px !important;');
      }
    });
  }
}

function createParallax()
{
  if (typeof window.orientation !== 'undefined' || $('body[id*="page-page.videos"]').length) { return; }
  
