.nav-search
{
  float: right;
  padding: 39px 0 3px 0;
  margin: 0;
}

.nav-search .nav-add-panel
{
  padding: 0;
}

.nav-search .nav-add-panel .searchwrap
{
  display: block;
}

.nav-search .nav-add-panel .searchwrap .form-wrap
{
  background: 0;
}

.nav-search .nav-add-panel .searchwrap .submit
{
  float: left;
  width: 57px;
  height: 57px;
  position: relative;
}

.nav-search .nav-add-panel .searchwrap .submit i
{
  position: absolute;
  font-size: 40px;
  line-height: 40px;
  font-size: 30px;
  left: 15px;
  top: 7px;
  color: #ffffff;
  cursor: pointer;
}

.nav-search .nav-add-panel .searchwrap .submit input
{
  text-indent: -9999px;
  background: 0;
  outline: 0;
  min-width: 57px;
  width: 57px;
  height: 57px;
}

.nav-search .search-input
{
  float: right;
}

.nav-search .search-input input[type="text"]
{
  background: 0;
  color: #ffffff;
  outline: 0;
  box-shadow: none;
  width: auto;
  height: 40px;
  padding: 0 35px 0 10px;
  border-bottom: 1px solid #ffffff;
  margin: 2px 0 0 0;
}

.nav-search .search-input .search-close
{
  position: relative;
  width: 25px;
  height: 25px;
  left: 100%;
  bottom: 30px;
  margin: 0 0 0 -30px;
  cursor: pointer;
}

.nav-search .search-input .search-close:before, .nav-search .search-input .search-close:after
{
  content: "";
  position: absolute;
  background: #ffffff;
  height: 1px;
  top: 50%;
  left: 0;
  right: 0;
  -webkit-transition: all .2s linear;
  transition: all .2s linear;
}

.nav-search .search-input .search-close:before
{
  -webkit-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  transform: rotate(45deg);
}

.nav-search .search-input .search-close:after
{
  -webkit-transform: rotate(-45deg);
  -ms-transform: rotate(-45deg);
  transform: rotate(-45deg);
}

.nav-search ::-webkit-input-placeholder
{
  color: #ffffff;
}

.nav-search ::-moz-placeholder
{
  color: #ffffff;
}

.nav-search :-moz-placeholder
{
  color: #ffffff;
}

.nav-search :-ms-input-placeholder
{
  color: #ffffff;
}

