  #mobile-menu-bar .logo, #mobile-menu-bar .my-account, #mobile-menu-bar .nav-opener, #mobile-menu-bar .minicart
  {
    float: left;
    margin: 10px 16px;
  }
  
  #mobile-menu-bar a
  {
    color: #ffffff;
  }
  
  #mobile-menu-bar i
  {
    font-size: 30px;
  }
}

@media only screen and (max-width: 767px)
{
