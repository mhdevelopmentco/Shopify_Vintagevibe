jQuery(document).ready(function($){
  
  var promo = $('.promo-block');
  var promoImg = promo.find('img');
  var promoImgsrc = promoImg.attr('src') + '&' + new Date().getTime();
  promo.css('background-image', 'url(' + promoImgsrc + ')')
  promoImg.attr('src', promoImgsrc);
  
  promoImg.one('error', function()
  {
    $(this).closest('.promo-block').css('background-image', 'url({{ 'img-collection-default.jpg' | asset_url }})');
    $(this).unbind('error').attr('src', '{{ 'img-collection-default.jpg' | asset_url }}');
  });
  
  promoImg.one('load', function()
  {
    createBanner();
    createParallax();
    createTextFade();
  });

function createBanner()
{
  var promo = $('.promo-block');
  var imageUrl = promo.css('background-image').match(/^url\("?(.+?)"?\)$/);
  if (imageUrl[1])
  {
    imageUrl = imageUrl[1];
    var image = new Image();
    image.src = imageUrl;
      
    $(image).one('load', function()
    {
      if (!$('body[id*="page-page.videos"]').length)
        adjustBanner(image.width, image.height);
      promo.css('visibility', '');
        
      $(window).on('resize', function()
      {
        adjustBanner(image.width, image.height);
      });
        
      function adjustBanner(width, height)
      {
        var minWidth = promo.width();
        var minHeight = promo.height();
          
        height = minWidth*(height/width);
        width = minWidth;
          
        if (height < minHeight)
        {
          width = minHeight*(width/height);
          height = minHeight;
        }
          
        if (!$('body[id*="page-col"]').length)
        {
          width  = (height+60)*(width/height);
          height = (height+60);
        }
          
        width = Math.floor(width);
        height = Math.floor(height);
          
        promo.css('background-size', '');
        promo.css('cssText', promo.attr('style') + 'background-size: ' + width + 'px ' + height + 'px !important;');
      }
    });
  }
}

function createParallax()
{
  if (typeof window.orientation !== 'undefined' || $('body[id*="page-page.videos"]').length) { return; }
  
  $('.promo-block-container').css({
   'height' : parseInt($('.promo-block').css('height')),
   'min-height' : parseInt($('.promo-block').css('min-height'))
  });

  var lastScrollY = 0,
      ticking = false,
      bgElm = $('.promo-block')[0],
      pageTitle = $('.promo-block .page-title')[0],
      offsetY = 59;
      speedDivider = 7.5;

  // Update background position
  var updatePosition = function() {
    var translateValue = lastScrollY / speedDivider;

    // We don't want parallax to happen if scrollpos is below 0
    if (translateValue < 0)
      translateValue = 0;

    translateY(bgElm, translateValue);
    translateY(pageTitle, -(translateValue));

    // Stop ticking
    ticking = false;
  };

  // Translates an element on the Y axis using translate3d to ensure
  // that the rendering is done by the GPU
  var translateY = function(elm, value) {
    var translate = 'translate3d(0px,' + value + 'px, 0px)';
    elm.style['-webkit-transform'] = translate;
    elm.style['-moz-transform'] = translate;
    elm.style['-ms-transform'] = translate;
    elm.style['-o-transform'] = translate;
    elm.style.transform = translate;
  };

  // This will limit the calculation of the background position to
  // 60fps as well as blocking it from running multiple times at once
  var requestTick = function() {
    if (!ticking) {
      window.requestAnimationFrame(updatePosition);
      ticking = true;
    }
  };

  // Update scroll value and request tick
  var doScroll = function() {
    lastScrollY = window.pageYOffset - offsetY;
    requestTick();
  };

  // Initialize on domready
  (function() {
    var loaded = 0;
    var bootstrap = function() {
      if (loaded) return;
      loaded = 1;

      rafPolyfill();
      window.onscroll = doScroll;
    };

    if ( document.readyState === 'complete' ) {
      setTimeout( bootstrap );
    } else {
      document.addEventListener( 'DOMContentLoaded', bootstrap, false );
      window.addEventListener( 'load', bootstrap, false );
    }
  })();

  // RequestAnimationFrame polyfill for older browsers
  var rafPolyfill = function() {
    var lastTime, vendors, x;
    lastTime = 0;
    vendors = ["webkit", "moz"];
    x = 0;
    while (x < vendors.length && !window.requestAnimationFrame) {
      window.requestAnimationFrame = window[vendors[x] + "RequestAnimationFrame"];
      window.cancelAnimationFrame = window[vendors[x] + "CancelAnimationFrame"] || window[vendors[x] + "CancelRequestAnimationFrame"];
      ++x;
    }
    if (!window.requestAnimationFrame) {
      window.requestAnimationFrame = function(callback, element) {
        var currTime, id, timeToCall;
        currTime = new Date().getTime();
        timeToCall = Math.max(0, 16 - (currTime - lastTime));
        id = window.setTimeout(function() {
          callback(currTime + timeToCall);
        }, timeToCall);
        lastTime = currTime + timeToCall;
        return id;
      };
    }
    if (!window.cancelAnimationFrame) {
      window.cancelAnimationFrame = function(id) {
        clearTimeout(id);
      };
    }
  };
}

