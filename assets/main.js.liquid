// init toggle search
function initToggleSearch(){
  var activeClass = 'search-expanded';
  jQuery('.nav-panel, .nav-add-panel').each(function(){
    var hold = jQuery(this);
    var form = hold.find('.searchwrap form');
    var input = form.find('input[type="text"]');
    var button = form.find('input[type="submit"]');
    var focushandler = function(){
    	hold.addClass(activeClass);
    };
    var clickHandler = function(e){
      if (!hold.hasClass(activeClass)) {
      	input.focus();
      	e.preventDefault();
      }
    };

    // attach events
    button.on('click', clickHandler);
    input.on('focus', focushandler);
    jQuery(document).on('click touchstart',function(e) {
	  if (jQuery(e.target).closest(form).length) return;
      hold.removeClass(activeClass);
      input.blur();
	  e.stopPropagation();
	});
  });
}

// init select language
function initSelectLanguage(){
	var activeClass = 'popup-active';
	jQuery('.links').each(function(){
		var hold = jQuery(this);
		var select = hold.find('#currencies');
		var options = select.find('option');
		var currentVal = select.val().toUpperCase();
		var box = hold.find('.lang');
		var drop = box.children('ul');
		var links = drop.find('a');
		var opener = box.find('.lang-active');
		var img = opener.find('img');
		var imgSrc = opener.attr('src');
		function refreshImage(newSrc, val){
			var tmpImg = new Image();
			tmpImg.onload = function() {
				opener.text(' '+val).prepend(img);
			};
			tmpImg.onerror = function(){
				opener.text(' '+val);
			};
			tmpImg.src = newSrc;
		}
		links.each(function(){
			var link = jQuery(this);
			var newSrc = link.attr('href');
			var val = link.text().toUpperCase();
			var clickHandler = function(e){
				e.preventDefault();
				var index = options.index(options.filter('[value="'+ val +'"]'));
				links.show();
				link.hide();
				select.prop('selectedIndex', index);
				select.change();
				box.removeClass(activeClass);
				drop.hide();
				img.attr('src', newSrc);
				refreshImage(newSrc, val);
			};
			if (val === currentVal) {
				links.show();
				link.hide();
				img.attr('src', newSrc);
				refreshImage(newSrc, val);
			}
			// attach events
			link.on('click', clickHandler);
		});
	});
}

// init card popup
function initCardPopup(){
  var activeClass = 'active';
  var animSpeed = 500;
  jQuery('#minicart-holder a').each(function(){
    	var link = jQuery(this);
    	var popup = jQuery(link.attr('href'));
    
        link.on('click', function(e){
          e.preventDefault();
          if (popup.hasClass(activeClass)){
            popup.slideUp({
              duration: animSpeed,
              complete: function(){
               	popup.removeClass(activeClass); 	
              }
            });
          }else {
            popup.slideDown({
              duration: animSpeed,
              complete: function(){
               	popup.addClass(activeClass);
              }
            });
          }
        });
  });
}

// init build tabs
function initBuildTabs(){
  var activeClass = 'active';
  jQuery('.description .content-block').each(function(){
    var tabArea = jQuery(this);
    tabArea.prepend(jQuery('<ul class="tabs"></ul>'));
    var tabContentBlocks = tabArea.find('.jstab');
    tabContentBlocks.each(function(ind,el){
      jQuery(el).attr('id','tab-'+ind);      
      tabArea.find('.tabs').append('<li><a href="#tab-'+ind+'">'+jQuery(el).find('h2').text()+'</a></li>');      
    });
    tabArea.find('ul.tabs li').eq(0).addClass(activeClass);
	tabArea.find('ul.tabs').contentTabs({
		addToParent: true,
		tabLinks: 'a'
	});
  });
}

