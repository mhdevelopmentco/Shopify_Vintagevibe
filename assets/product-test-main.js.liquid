/* scripts.js */
{% comment %}
jQuery(document).ready(function($){
	$(window).on('load resize', function(){
		adjustImagesHeight();
	});
	function adjustImagesHeight(){
		var maxHeight = 0;
		$('.features-box .feature').each(function() {
			$(this).css('height', '');
			maxHeight = Math.max(maxHeight, $(this).height());
		});
		$('.features-box .feature').css('height', maxHeight + 'px');
	}
	var tracks = [];
	soundManager.setup({
		onready: function() {
			$('.playlist .track-list li').each(function(index) {
				var id = 'track' + index;
				var url = $(this).attr('data-track-url');
				var lengthItem = $(this).find('.length');
				tracks.push(soundManager.createSound({
					id: id,
					url: url,
					autoLoad: true,
					onload: function() {
						lengthItem.text(millisToMinutesAndSeconds(this.duration));
					},
					onfinish: function() {
						var newIndex = $('.playlist .track-list li.active').index() + 1;
						if (newIndex < tracks.length) $('.playlist .track-list li:eq(' + newIndex + ') a.play-button').click();
					}
				}));
			});
		}
	});
	$('.audio-bar .back-button').on('click', function() {
		soundManager.setPosition(getTrackID(), 0);
	});
	$('.audio-bar .play-button').on('click', function() {
		soundManager.play(getTrackID());
		$(this).hide();
		$(this).next('.pause-button').show();
		$('.playlist .track-list li.active a.play-button').hide();
		$('.playlist .track-list li.active a.pause-button').show();
	});
	$('.audio-bar .pause-button').on('click', function() {
		soundManager.pause(getTrackID());
		$(this).hide();
		$(this).prev('.play-button').show();
		$('.playlist .track-list li.active a.pause-button').hide();
		$('.playlist .track-list li.active a.play-button').show();
	});
	$('.audio-bar .forward-button').on('click', function() {
		soundManager.setPosition(getTrackID(), 0);
	});
	$('.audio-bar .note').on('click', function() {
		if ($(window).width() < 753)
          jQuery('.playlist').slideToggle();
	});
    $('.audio-bar .note').hover(function() {
		$('.playlist').addClass('hs-tooltip-visible');
	}, function() {
		setTimeout(function(){
           if (!$('.playlist:hover').length) { $('.playlist').removeClass('hs-tooltip-visible'); }
         }, 20);
	});
    $('.playlist').hover(function() {
	}, function() {
		$('.playlist').removeClass('hs-tooltip-visible');
	});
    $(window).on('load resize', function() {
		if ($(window).width() > 752)
          jQuery('.playlist').show();
        else
          jQuery('.playlist').hide();
	});
	$('.playlist .track-list li a.play-button').on('click', function() {
		$('.playlist .track-list li.active').removeClass('active');
		$(this).closest('li').addClass('active');
		$('.playlist .track-list li a.pause-button').hide();
		$('.playlist .track-list li a.play-button').show();
		$(this).hide();
		$(this).next('.pause-button').show();
		$('.audio-bar .play-button').hide();
		$('.audio-bar .pause-button').show();
		$('.audio-bar .title').text($(this).closest('.track').text());
		soundManager.pauseAll();
		//soundManager.setPosition(getTrackID(), 0);
		soundManager.play(getTrackID());
	});
	$('.playlist .track-list li a.pause-button').on('click', function() {
		$('.playlist .track-list li.active').removeClass('active');
		$(this).closest('li').addClass('active');
		$('.playlist .track-list li a.pause-button').hide();
		$('.playlist .track-list li a.play-button').show();
		$(this).hide();
		$(this).prev('.play-button').show();
		$('.audio-bar .pause-button').hide();
		$('.audio-bar .play-button').show();
		$('.audio-bar .title').text($(this).closest('.track').text());
		soundManager.pauseAll();
	});
	function getTrackID() {
		return 'track' + $('.playlist .track-list li.active').index();
	}
	function millisToMinutesAndSeconds(millis) {
		var minutes = Math.floor(millis / 60000);
		var seconds = ((millis % 60000) / 1000).toFixed(0);
		return minutes + ":" + (seconds < 10 ? '0' : '') + seconds;
	}
});
{% endcomment %}




