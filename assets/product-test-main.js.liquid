function initAnchors() {
	new SmoothScroll({
		activeClasses: 'link',
		anchorActiveClass: 'active',
		extraOffset: function() {
			var totalHeight = 0;
			jQuery('.product-subnav-section').each(function(){
				totalHeight += jQuery(this).outerHeight();
			});
			return totalHeight;
		},
		anchorLinks: '.product-subnav a[href^="#"]:not([href="#"])'
	});
}

function initLayoutFix() {
	var win = jQuery(window);
	var page = jQuery('html');
	var fixedClass = 'page-fixed';

	jQuery('.video-section').each(function() {
		var block = jQuery(this);
		var iframe = block.find('iframe');
		var activeClass = 'video-visible';
		var playVideo = function(e) {
			e.preventDefault();
			if (!block.hasClass(activeClass)) {
				block.addClass(activeClass);
				iframe[0].contentWindow.postMessage('{"event":"command","func":"playVideo","args":""}','*');
				iframe[0].contentWindow.postMessage('{"method":"play","value":""}', '*');
			}
		};
		var pauseVideo = function(e) {
			e.preventDefault();
			if (block.hasClass(activeClass)) {
				block.removeClass(activeClass);
				iframe[0].contentWindow.postMessage('{"event":"command","func":"pauseVideo","args":""}','*');
				iframe[0].contentWindow.postMessage('{"method":"pause","value":""}', '*');
			}
		};
		block.on('click', '.play-video', playVideo);
		block.on('click', '.close', pauseVideo);
	});

	win.on('load resize orientationchange scroll', function() {
		if (win.scrollTop() > 0) {
			page.addClass(fixedClass);
		} else {
			page.removeClass(fixedClass);
		}
	});
	jQuery('.product-subnav-section').each(function() {
		var bar = jQuery(this);
		var header = jQuery('.add-panel-container');
		var fixedStateClass = 'fixed-state';
		var fixedPosition;

		var resizeHandler = function() {
			bar.removeClass(fixedStateClass);
			fixedPosition = bar.offset().top;
			header.css({
				top: ''
			});
			scrollHandler();
		};

		var scrollHandler = function() {
			var winScroll = win.scrollTop();
			var headerHeight = header.outerHeight();
			if (winScroll >= fixedPosition) {
				bar.addClass(fixedStateClass);
			} else {
				bar.removeClass(fixedStateClass);
			}
			if (winScroll >= fixedPosition - headerHeight) {
				header.css({ top:- Math.max(0, Math.min(winScroll - (fixedPosition - headerHeight), headerHeight)) });
			} else {
				header.css({ top: '' });
			}
		};

		resizeHandler();
		win.on('resize load orientationchange', resizeHandler);
		win.on('scroll', scrollHandler);
	});
}

