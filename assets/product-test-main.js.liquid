		block.mediaBoxes({
			overlayEffect: 'push-up',
			columns: 3,
			lazyLoad: true,
			boxesToLoadStart: block.data('start') || 5,
			boxesToLoad: block.data('load') || 3,
			horizontalSpaceBetweenBoxes: 15,
			verticalSpaceBetweenBoxes: 15,
			LoadingWord: '',
			loadMoreWord: '',
			noMoreEntriesWord: ''
		});
		block.on( 'layoutComplete', function() {
			block.find('img').not('[data-action]').attr('data-action', 'zoom');
		});
	});
}

