/* cycle carousel basic styles */



.cycle-gallery {
  position: relative;
  max-width: 1360px;
  margin: 0 auto;
  z-index: 5;
}

.cycle-gallery .mask {
  position: relative;
  overflow: hidden;
  width: 100%;
}

.cycle-gallery .slideset {
  width: 99999px;
  float: left;
}

.cycle-gallery .slide {
  float: left;
}

.cycle-gallery .slide.active {
  z-index: 1;
}

.cycle-gallery .slide .text {
  position: absolute;
  bottom: 30px;
  left: 26px;
  right: 150px;
  color: #fff;
  font-size: 23px;
  line-height: 26px;
  text-align: left;
  font-weight: normal;
}

.cycle-gallery .slide img {
  width: 100%;
  height: auto;
  display: block;
}

.cycle-gallery .btn-prev,
.cycle-gallery .btn-next {
  left: auto;
  top: auto;
  bottom: 7px;
  right: 7px;
  background: #464646 url({{ 'btn-next.png' | asset_url }}) no-repeat 50% 50%;
  color: #fff;
  width: 67px;
  height: 67px;
}

.cycle-gallery .btn-prev:hover,
.cycle-gallery .btn-next:hover {
  background-position: 50% 50%;
  opacity: .6;
}

.cycle-gallery .btn-prev {
  right: 76px;
  background: #464646 url({{ 'btn-prev.png' | asset_url }}) no-repeat 50% 50%;
}

.accessibility {
  top: -100px;
}

.bg-stretch {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: -1;
  background-size: cover;
  background-position: 50% 50%;
}

.product-block-enhanced .container {
  max-width: 1470px;
  padding: 0 37px;
}

.product-block-enhanced .top-section {
  position: relative;
