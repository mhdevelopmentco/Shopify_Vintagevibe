/* zoom.css */

img[data-action="zoom"] {
  cursor: pointer;
  cursor: -webkit-zoom-in;
  cursor: -moz-zoom-in;
}

.zoom-img,
.zoom-img-wrap {
  position: relative;
  z-index: 666;
  -webkit-transition: all 300ms;
  transition: all 300ms;
}

img.zoom-img {
  cursor: pointer;
  cursor: -webkit-zoom-out;
  cursor: -moz-zoom-out;
}

.zoom-overlay {
  z-index: 420;
  background: #fff;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  pointer-events: none;
  filter: "alpha(opacity=0)";
  opacity: 0;
  -webkit-transition: opacity 300ms;
  transition: opacity 300ms;
}

.zoom-overlay-open .zoom-overlay {
  filter: "alpha(opacity=100)";
  opacity: 1;
}

.zoom-overlay-open,
.zoom-overlay-transitioning {
  cursor: default;
}





/* playlist.css */

@media (min-width: 768px) {

  .playlist {
    opacity: 0;
    transition-property: opacity;
    background: #ffffff;
    bottom: calc(100% + 20px);
    right: 0;
    width: 100%;
    height: auto;
    padding: 30px 30px 20px;
    border-radius: 5px;
  }

  .playlist:after {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: calc(100% + 24px);
  }

  .playlist .hs-arrow.hs-arrow-bottom {
    left: auto;
    right: 55px;
    border-top-color: #ffffff;
  }

  .playlist .container {
    padding: 0 !important;
  }

  .playlist .header {
    font-size: 14px;
    font-weight: bold;
    padding: 0 0 30px 0;
  }

  .playlist .header .track, .playlist .header .length {
    color: #ec1100;
    padding: 0;
  }

  .playlist .track {
    display: inline-block;
    position: relative;
    width: 380px;
    padding: 0 0 0 30px;
  }

  .playlist .header .length {
    display: none;
    padding: 0 0 0 5px;
  }

  .playlist .length {
    display: inline-block;
  }

  .playlist .track-list {
    list-style: outside none none;
    padding: 0;
    margin: 0;
  }

  .playlist .track-list li {
    font-size: 14px;
    padding: 0 0 20px 0;
  }

  .playlist .track-list .length {
    width: 46px;
    padding: 0 0 0 10px;
    text-align: right;
  }

  .playlist .play-button,.playlist .pause-button {
    position: absolute;
    top: 0;
    left: 0;
    background-position: 50% 50%;
    width: 20px;
    height: 20px;
    z-index: 1;
  }

  .playlist .play-button {
    background-image: url(//cdn.shopify.com/s/files/1/0666/2821/files/play-button.png?15263470485060733658);
  }

  .playlist .pause-button {
    background-image: url(//cdn.shopify.com/s/files/1/0666/2821/files/pause-button.png?15263470485060733658);
  }

  .playlist .play-button:hover,.playlist .pause-button:hover {
    -webkit-filter: invert(50%);
    filter: invert(50%);
  }
}

@media (max-width: 767px) {

  .playlist {
    background: rgba(92,92,92,0.9);
    width: 100%;
    padding: 20px;
    z-index: 3;
  }

  .playlist .container {
    padding: 0 !important;
  }

  .playlist .header {
    font-size: 20px;
    line-height: 23px;
    color: #ffffff;
    padding: 0 0 10px 12px;
  }

  .playlist .track {
    display: inline-block;
    position: relative;
    padding: 0 0 0 50px;
  }

  .playlist .length {
    display: inline-block;
  }

  .playlist .track-list {
    list-style: outside none none;
    padding: 0;
    border-top: 2px solid #ccc;
    border-bottom: 2px solid #ccc;
    margin: 0;
  }

  .playlist .track-list li {
    font-size: 14px;
    line-height: 17px;
    color: #ffffff;
    padding: 15px 0;
    border-bottom: 1px solid #b6b6b6;
  }

  .playlist .track {
    width: 75%;
  }

  .playlist .track-list .length {
    padding: 0 0 0 10px;
  }

  .playlist .play-button, .playlist .pause-button {
    position: absolute;
    top: 0;
    left: 10px;
    background-position: 50% 50%;
    width: 20px;
    height: 20px;
  }

  .playlist .play-button {
    background-image: url(//cdn.shopify.com/s/files/1/0666/2821/files/play-button.png?15263470485060733658);
  }

  .playlist .pause-button {
    background-image: url(//cdn.shopify.com/s/files/1/0666/2821/files/pause-button.png?15263470485060733658);
  }

  .playlist .play-button,.playlist .pause-button {
    -webkit-filter: invert(50%);
    filter: invert(50%);
  }

  .playlist .play-button:hover,.playlist .pause-button:hover {
    -webkit-filter: invert(0%);
    filter: invert(0%);
  }
}





