$('.tp-banner li').click(function()
{
  if ($(window).width() < 767) { window.location = $(this).find('a').attr('href'); }
});
$('.cycle-gallery .slide, .info-section .info-col, .info-section2 .info-col, .add-boxes .info-col').click(function()
{
  window.location = $(this).find('a').attr('href');
});
    
$('a.nav-opener').click(function()
{
  if ( !$(this).parent().hasClass('nav-active') )
    $('html, body').animate({ scrollTop: $(this).offset().top }, 400);
});


jQuery(document).ready(function($){
  
  $('.categories-nav ul li.active').parent().parent().parent().addClass('active');
  
  if ($('body').attr('id') != 'page-index') { return; }
  
  $(window).load(function()
  {
    adjustMargin();
  });
  
  $(window).on('resize', function()
  {
    adjustMargin();
  });
  
  function adjustMargin()
  {
    if ($(window).width() > 1008)
    {
      $('.info-col').each(function()
      {
        var col = $(this);
        var tab = $(this).parent().find('.tabs-box');
        if (col.height() >= tab.height())
        {
          tab.css('margin-top', (col.height() - tab.height()) + 'px');
          col.css('margin-top', '0px');
        }
        else (tab.height() > col.height())
        {
          col.css('margin-top', (tab.height() - col.height()) + 'px');
          tab.css('margin-top', '0px');
        }
      });
    }
    else
    {
      $('.info-col').each(function()
      {
        var col = $(this);
        var tab = $(this).parent().find('.tabs-box');
        col.css('margin-top', '0px');
        tab.css('margin-top', '0px');
      });
    }
  }
  
});





