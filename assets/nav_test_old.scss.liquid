}

.nav-add-panel .nav-opener
{
  display: block;
  float: right;
  position: relative;
  background: 0;
  width: 40px;
  height: 40px;
  /*margin: 55px 0;*/
  margin: 47px -10px 47px 0;
  text-indent: -9999px;
}

.nav-add-panel .nav-opener:hover
{
  opacity: .9;
}

.nav-add-panel .nav-opener:before, .nav-add-panel .nav-opener:after, .nav-add-panel .nav-opener span
{
  background: #ffffff;
  position: absolute;
  top: 12px;
  left: 25%;
  right: 25%;
  height: 3px;
  border-radius: 4px;
  margin-top: -2px;
  -webkit-transition: all .2s linear;
  transition: all .2s linear;
}

.nav-add-panel .nav-opener:before, .nav-add-panel .nav-opener:after
{
  content: "";
  top: 21px;
}

.nav-add-panel .nav-opener:after
{
  top: 30px;
}

.nav-holder.desktop
{
  float: right;
}

.nav-add-panel .nav-holder.mobile
{
  position: absolute;
  overflow: hidden;
  max-height: 0;
  top: 100%;
  left: 0;
  right: 0;
  -webkit-transition: all .25s linear;
  transition: all .25s linear;
}

.resize-active .nav-add-panel .nav-holder.mobile
{
  -webkit-transition: none;
  transition: none;
}

.nav-add-panel .nav-holder.mobile .nav-frame
{
  -webkit-transform: translateY(-100%);
  -ms-transform: translateY(-100%);
  transform: translateY(-100%);
  -webkit-transition: all .25s linear;
  transition: all .25s linear;
}

.nav-add-panel.nav-active .nav-opener span
{
  opacity: 0;
}

.nav-add-panel.nav-active .nav-opener:before, .nav-add-panel.nav-active .nav-opener:after
{
  top: 21px;
  left: 15%;
  right: 15%;
  -webkit-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  transform: rotate(45deg);
}

.nav-add-panel.nav-active .nav-opener:after
{
  -webkit-transform: rotate(-45deg);
  -ms-transform: rotate(-45deg);
  transform: rotate(-45deg);
}

