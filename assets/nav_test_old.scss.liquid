}

.nav-search .nav-add-panel .searchwrap .submit i
{
  position: absolute;
  font-size: 40px;
  line-height: 40px;
  font-size: 30px;
  left: 15px;
  top: 7px;
  color: #ffffff;
  cursor: pointer;
}

.nav-search .nav-add-panel .searchwrap .submit input
{
  text-indent: -9999px;
